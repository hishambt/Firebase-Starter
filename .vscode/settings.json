		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#8a238d",
		"statusBar.background": "#621964",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#8a238d",
		"statusBarItem.remoteBackground": "#621964",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#621964",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#62196499",
		"titleBar.inactiveForeground": "#e7e7e799"
