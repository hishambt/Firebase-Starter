{
	"name": "firebase-starter",
	"version": "0.0.1",
	"author": "Ionic Framework",
	"homepage": "https://ionicframework.com/",
	"scripts": {
		"ng": "ng",
		"ionic": "ionic",
		"firebase": "firebase",
		"start:lib": "concurrently \"ng build softside-ui --watch\" \"tailwind --watch -c ./projects/softside-ui/tailwind.config.js -o ./projects/softside-ui/lib/css/tailwind.scss -p\"",
		"start:web": "kill-port 9099 8080 5000 9199 4000 5001 && firebase emulators:exec --project=demo-project --ui \"ionic serve --project web\" --import=./emulators.backup --export-on-exit=./emulators.backup",
		"start:mobile": "kill-port 9099 8080 5000 9199 4000 5001 && firebase emulators:exec --project=demo-project --ui \"ionic serve --project mobile\" --import=./emulators.backup --export-on-exit=./emulators.backup",
		"build:lib": "ng build softside-ui && tailwind -c ./projects/softside-ui/tailwind.config.js -o ./dist/softside-ui/css/tailwind.scss",
		"build:web": "env-cmd -e production_web ng build web",
		"build:mobile": "env-cmd -e production_mobile ng build mobile",
		"deploy:local": "servor public index.html 4321 --browse --secure",
		"test": "ng test",
		"lint": "ng lint",
		"prepare": "husky install && shx cp -urf .husky\\* .git\\hooks\\",
		"stylelint": "stylelint",
		"lint:styles:lib": "stylelint \"dist/softside-ui/css/tailwind.scss\" --fix",
		"lint:styles": "stylelint \"projects/**/*.scss\"",
		"lint:styles-fix": "stylelint \"projects/**/*.scss\" --fix",
		"prettier:ts": "npx prettier --write projects/**/*.ts",
		"prettier:html": "npx prettier --write projects/**/*.html",
		"prettier:scss": "npx prettier --write projects/**/*.scss",
		"prettier:all": "npx prettier --write projects/**/*.{ts,html,scss}",
		"docs:lib:storybook": "ng run lib:storybook",
		"docs:lib:storybook-documentation": "npx compodoc -p projects/lib/.storybook/tsconfig.json -e json -d projects/lib",
		"docs:lib:build-storybook": "ng run lib:build-storybook",
		"docs:lib:compodoc": "npx compodoc -p projects/lib/tsconfig.doc.json -d docs/lib -s --port 8085",
		"docs:mobile:storybook": "ng run mobile:storybook",
		"docs:mobile:storybook-documentation": "npx compodoc -p projects/mobile/.storybook/tsconfig.json -e json -d projects/mobile",
		"docs:mobile:build-storybook": "ng run mobile:build-storybook",
		"docs:mobile:compodoc": "npx compodoc -p projects/mobile/tsconfig.doc.json -d docs/mobile -s --port 8086",
		"docs:web:storybook": "ng run web:storybook",
		"docs:web:storybook-documentation": "npx compodoc -p projects/web/.storybook/tsconfig.json -e json -d projects/web",
		"docs:web:build-storybook": "ng run web:build-storybook",
		"docs:web:compodoc": "npx compodoc -p projects/web/tsconfig.doc.json -d docs/web -s --port 8087"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.0.0",
		"@angular/common": "^16.0.0",
		"@angular/compiler": "^16.0.0",
		"@angular/core": "^16.0.0",
		"@angular/fire": "^7.6.1",
		"@angular/forms": "^16.0.0",
		"@angular/platform-browser": "^16.0.0",
		"@angular/platform-browser-dynamic": "^16.0.0",
		"@angular/router": "^16.0.0",
		"@capacitor/app": "5.0.6",
		"@capacitor/core": "5.3.0",
		"@capacitor/haptics": "5.0.6",
		"@capacitor/keyboard": "5.0.6",
		"@capacitor/status-bar": "5.0.6",
		"@ionic/angular": "^7.0.0",
		"ionicons": "^7.0.0",
		"ng-let": "^16.0.2",
		"ngx-image-cropper": "^7.0.2",
		"ngx-softside-table": "^1.0.0",
		"rxjs": "~7.8.0",
		"softside-ui": "file:dist/softside-ui",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^16.0.0",
		"@angular-eslint/builder": "16.1.2",
		"@angular-eslint/eslint-plugin": "16.1.2",
		"@angular-eslint/eslint-plugin-template": "16.1.2",
		"@angular-eslint/schematics": "16.1.2",
		"@angular-eslint/template-parser": "16.1.2",
		"@angular/cli": "^16.0.0",
		"@angular/compiler": "^16.0.0",
		"@angular/compiler-cli": "^16.0.0",
		"@angular/language-service": "^16.0.0",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@capacitor/cli": "5.3.0",
		"@commitlint/cli": "^17.7.1",
		"@compodoc/compodoc": "^1.1.17",
		"@ionic/angular-toolkit": "^9.0.0",
		"@ionic/cli": "^7.1.1",
		"@storybook/addon-docs": "^7.4.1",
		"@storybook/addon-essentials": "^7.4.1",
		"@storybook/addon-interactions": "^7.4.1",
		"@storybook/addon-links": "^7.4.1",
		"@storybook/angular": "^7.4.1",
		"@storybook/blocks": "^7.4.1",
		"@storybook/testing-library": "^0.2.0",
		"@types/jasmine": "~4.3.0",
		"@types/node": "^18.17.15",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"autoprefixer": "^10.4.7",
		"commitlint-config-jira": "^1.6.4",
		"commitlint-plugin-jira-rules": "^1.6.4",
		"concurrently": "^8.2.1",
		"env-cmd": "^10.1.0",
		"eslint": "^8.47.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-deprecation": "^1.5.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-unused-imports": "^2.0.0",
		"firebase-tools": "^12.5.3",
		"husky": "^8.0.3",
		"jasmine-core": "~4.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"kill-port": "^2.0.1",
		"lint-staged": "^14.0.1",
		"ng-packagr": "^16.2.3",
		"postcss": "^8.4.14",
		"prettier": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"servor": "^4.0.2",
		"shx": "^0.3.4",
		"storybook": "^7.4.1",
		"stylelint": "^15.10.3",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-scss": "^5.1.0",
		"tailwindcss": "^3.1.6",
		"ts-node": "^8.3.0",
		"typescript": "~5.0.2"
	},
	"description": "An Ionic project",
	"lint-staged": {
		"*.ts": [
			"eslint"
		],
		"*.{.css,scss}": "stylelint --fix"
	}
}
