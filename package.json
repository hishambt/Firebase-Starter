{
	"name": "cv-generator",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"ng-help": "ng --help",
		"serve": "ng serve",
		"start": "kill-port 9099 8080 5000 9199 4000 && firebase emulators:exec --project=demo-project --ui \"ng serve\" --import=./emulators.backup --export-on-exit=./emulators.backup",
		"pretty-ts": "npx prettier --write src/app/**/*.ts",
		"pretty-html": "npx prettier --write src/app/**/*.html",
		"pretty-scss": "npx prettier --write src/app/**/*.scss",
		"pretty-all": "npx prettier --write src/app/**/*.{ts,html,scss}",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint",
		"lint:styles": "stylelint \"src/**/*.scss\"",
		"lint:styles-fix": "stylelint \"src/app/**/*.scss\" --fix"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.1.0",
		"@angular/cdk": "^16.1.1",
		"@angular/common": "^16.1.0",
		"@angular/compiler": "^16.1.0",
		"@angular/core": "^16.1.0",
		"@angular/fire": "^7.6.1",
		"@angular/forms": "^16.1.0",
		"@angular/material": "^16.1.1",
		"@angular/platform-browser": "^16.1.0",
		"@angular/platform-browser-dynamic": "^16.1.0",
		"@angular/router": "^16.1.0",
		"bootstrap": "~5.2.3",
		"ng-let": "^16.0.2",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^16.1.0",
		"@angular-eslint/builder": "16.1.1",
		"@angular-eslint/eslint-plugin": "16.1.1",
		"@angular-eslint/eslint-plugin-template": "16.1.1",
		"@angular-eslint/schematics": "16.1.1",
		"@angular-eslint/template-parser": "16.1.1",
		"@angular/cli": "~16.1.0",
		"@angular/compiler-cli": "^16.1.0",
		"@types/jasmine": "~4.3.0",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"eslint": "^8.47.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-deprecation": "^1.5.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"jasmine-core": "~4.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"kill-port": "^2.0.1",
		"stylelint": "^15.10.3",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-scss": "^5.1.0",
		"typescript": "~5.1.3"
	}
}
