{
	"name": "firebase-starter",
	"version": "0.0.1",
	"author": "Ionic Framework",
	"homepage": "https://ionicframework.com/",
	"scripts": {
		"ng": "ng",
		"ionic": "ionic",
		"start": "kill-port 9099 8080 5000 9199 4000 5001 && firebase emulators:exec --project=demo-project --ui \"ionic serve\" --import=./emulators.backup --export-on-exit=./emulators.backup",
		"firebase": "firebase",
		"angular:serve": "ng serve",
		"build": "ng build",
		"prepare": "husky install && xcopy /E /I /Y .husky\\* .git\\hooks\\",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint",
		"lint:styles": "stylelint \"projects/core-ui/src/**/*.scss\"",
		"lint:styles-fix": "stylelint \"projects/core-ui/src/app/**/*.scss\" --fix",
		"prettier:ts": "npx prettier --write projects/core-ui/src/app/**/*.ts",
		"prettier:html": "npx prettier --write projects/core-ui/src/app/**/*.html",
		"prettier:scss": "npx prettier --write projects/core-ui/src/app/**/*.scss",
		"prettier:all": "npx prettier --write projects/core-ui/src/app/**/*.{ts,html,scss}",
		"docs:storybook": "ng run core-ui:storybook",
		"docs:storybook-documentation": "npx compodoc -p projects/core-ui/.storybook/tsconfig.json -e json -d projects/core-ui",
		"docs:build-storybook": "ng run core-ui:build-storybook",
		"docs:compodoc": "npx compodoc -p projects/core-ui/tsconfig.doc.json -d docs/core-ui -s --port 8085"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.0.0",
		"@angular/common": "^16.0.0",
		"@angular/compiler": "^16.0.0",
		"@angular/core": "^16.0.0",
		"@angular/fire": "^7.6.1",
		"@angular/forms": "^16.0.0",
		"@angular/platform-browser": "^16.0.0",
		"@angular/platform-browser-dynamic": "^16.0.0",
		"@angular/router": "^16.0.0",
		"@capacitor/app": "5.0.6",
		"@capacitor/core": "5.3.0",
		"@capacitor/haptics": "5.0.6",
		"@capacitor/keyboard": "5.0.6",
		"@capacitor/status-bar": "5.0.6",
		"@ionic/angular": "^7.0.0",
		"ionicons": "^7.0.0",
		"ng-let": "^16.0.2",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^16.0.0",
		"@angular-eslint/builder": "16.1.2",
		"@angular-eslint/eslint-plugin": "16.1.2",
		"@angular-eslint/eslint-plugin-template": "16.1.2",
		"@angular-eslint/schematics": "16.1.2",
		"@angular-eslint/template-parser": "16.1.2",
		"@angular/cli": "^16.0.0",
		"@angular/compiler": "^16.0.0",
		"@angular/compiler-cli": "^16.0.0",
		"@angular/language-service": "^16.0.0",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@capacitor/cli": "5.3.0",
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@compodoc/compodoc": "^1.1.17",
		"@ionic/angular-toolkit": "^9.0.0",
		"@ionic/cli": "^7.1.1",
		"@storybook/addon-docs": "^7.4.1",
		"@storybook/addon-essentials": "^7.4.1",
		"@storybook/addon-interactions": "^7.4.1",
		"@storybook/addon-links": "^7.4.1",
		"@storybook/angular": "^7.4.1",
		"@storybook/blocks": "^7.4.1",
		"@storybook/testing-library": "^0.2.0",
		"@types/jasmine": "~4.3.0",
		"@types/node": "^18.17.15",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.47.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-deprecation": "^1.5.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-unused-imports": "^2.0.0",
		"firebase-tools": "^12.5.3",
		"husky": "^8.0.3",
		"jasmine-core": "~4.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"kill-port": "^2.0.1",
		"lint-staged": "^14.0.1",
		"postcss": "^8.4.14",
		"prettier": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.4.1",
		"stylelint": "^15.10.3",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-scss": "^5.1.0",
		"tailwindcss": "^3.1.6",
		"ts-node": "^8.3.0",
		"typescript": "~5.0.2"
	},
	"description": "An Ionic project",
	"lint-staged": {
		"*.ts": [
			"eslint"
		],
		"*.{.css,scss}": "stylelint --fix"
	}
}
