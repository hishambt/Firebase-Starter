{
	"name": "firebase-starter",
	"version": "0.0.1",
	"author": "Ionic Framework",
	"homepage": "https://ionicframework.com/",
	"scripts": {
		"ng": "ng",
		"ionic": "ionic",
		"start": "kill-port 9099 8080 5000 9199 4000 5001 && firebase emulators:exec --project=demo-project --ui \"ionic serve\" --import=./emulators.backup --export-on-exit=./emulators.backup",
		"firebase": "firebase",
		"angular:serve": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint",
		"lint:styles": "stylelint \"src/**/*.scss\"",
		"lint:styles-fix": "stylelint \"src/app/**/*.scss\" --fix",
		"pretty-ts": "npx prettier --write projects/core-ui/src/app/**/*.ts",
		"pretty-html": "npx prettier --write projects/core-ui/src/app/**/*.html",
		"pretty-scss": "npx prettier --write projects/core-ui/src/app/**/*.scss",
		"pretty-all": "npx prettier --write projects/core-ui/src/app/**/*.{ts,html,scss}"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.0.0",
